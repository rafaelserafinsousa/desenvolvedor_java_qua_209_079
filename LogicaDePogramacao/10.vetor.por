// 10 - vetor
programa {
  funcao inicio() {
    // declaração de valor
    cadeia nomes [] = {"fulano", "ciclano", "betrano", "joão", "maria"}

    // exibe na tela o primeiro nome da lista
    escreva(nomes[0],"\n")
    
    // exibe na tela a lista de nomes
    para (inteiro i = 0; i<5; i++) escreva(nomes[i], "\n")

  }
}
